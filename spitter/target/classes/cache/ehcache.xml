<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="ehcache.xsd"
         updateCheck="true"
         monitoring="autodetect"
         dynamicConfig="true">
    <diskStore path="java.io.tmpdir" />
    <defaultCache maxElementsInMemory="500" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" eternal="true"/>
    <cache name="spitterCache" maxElementsInMemory="500" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" eternal="true"/>
</ehcache>
