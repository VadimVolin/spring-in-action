<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:p="http://www.springframework.org/schema/p"
       xmlns:util="http://www.springframework.org/schema/util"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
						 http://www.springframework.org/schema/context
						 http://www.springframework.org/schema/context/spring-context-3.0.xsd
						http://www.springframework.org/schema/util
                    	http://www.springframework.org/schema/util/spring-util-2.5.xsd">

    <context:annotation-config/>

    <bean id="instrument" class="app.instrument.Saxophone"/>
    <bean id="instrument1" class="app.instrument.Saxophone"/>
    <bean id="instrument2" class="app.instrument.Saxophone"/>

    <bean id="kenny3" class="app.annotation.instrumental.Instrumentalist">
        <property name="name" value="kenny3"/>
        <property name="song" value="Annotation song"/>
    </bean>

</beans>