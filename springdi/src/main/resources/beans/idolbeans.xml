<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-init-method="turnOnLights"
	default-destroy-method="turnOffLights">

	<bean id="poeticDuke" class="app.juggler.PoeticJuggler">
		<constructor-arg value="15"></constructor-arg>
		<constructor-arg value=" poetic juggler"></constructor-arg>
		<constructor-arg>
			<bean id="sonnet29" class="app.sonnet.Sonnet29"></bean>
		</constructor-arg>
	</bean>

	<bean id="duke" class="app.juggler.Juggler">
		<constructor-arg value="'duke'"></constructor-arg>
	</bean>

	<bean id="duke1" class="app.juggler.Juggler">
		<constructor-arg value="duke1"></constructor-arg>
	</bean>

	<bean id="duke2" class="app.juggler.Juggler">
		<constructor-arg value="duke2"></constructor-arg>
	</bean>


	<bean id="theStage" class="app.stage.Stage"
		factory-method="getInstance"></bean>

	<bean id="auditorium" class="app.auditorium.Auditorium"></bean>



	<bean id="saxophone" class="app.instrument.Saxophone"></bean>
	<bean id="piano" class="app.instrument.Piano"></bean>

	<bean id="guitar" class="app.instrument.Guitar"></bean>
	<bean id="cymbal" class="app.instrument.Cymbal"></bean>
	<bean id="harmonica" class="app.instrument.Harmonica"></bean>

	<bean id="kenny" class="app.instrumental.Instrumentalist"
		p:song="Jingle Bells" p:instrument-ref="saxophone">
	</bean>

	<bean id="hank" class="app.instrumental.OneManBand">
		<property name="instruments">
			<props>
				<prop key="GUITAR">Strum strum strum</prop>
				<prop key="CYMBAL">tlink tlink tlink</prop>
				<prop key="HARMONICA">pram pram pram</prop>
			</props>
		</property>
	</bean>

	<bean id="carl" class="app.instrumental.Instrumentalist">
		<property name="song" value="#{kenny.song}"></property>
		<property name="instrument"
			value="#{(kenny.song eq 'Jingle Belllls')?saxophone:piano}"></property>
	</bean>

</beans>