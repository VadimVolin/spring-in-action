<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:p="http://www.springframework.org/schema/p"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
						http://www.springframework.org/schema/beans/spring-beans-3.0.xsd"
	default-init-method="turnOnLights"
	default-destroy-method="turnOffLights">

	<bean id="poeticDuke" class="app.juggler.PoeticJuggler">
		<constructor-arg value="15"></constructor-arg>
		<constructor-arg value=" poetic juggler"></constructor-arg>
		<constructor-arg>
			<bean id="sonnet29" class="app.sonnet.Sonnet29"></bean>
		</constructor-arg>
	</bean>

	<bean id="duke" class="app.juggler.Juggler">
		<constructor-arg value="'duke'"></constructor-arg>
	</bean>

	<bean id="duke1" class="app.juggler.Juggler">
		<constructor-arg value="duke1"></constructor-arg>
	</bean>

	<bean id="duke2" class="app.juggler.Juggler">
		<constructor-arg value="duke2"></constructor-arg>
	</bean>


	<bean id="theStage" class="app.stage.Stage"
		factory-method="getInstance"></bean>

	<bean id="auditorium" class="app.auditorium.Auditorium"></bean>



	<bean id="saxophone" class="app.instrument.Saxophone"></bean>
	<bean id="kenny" class="app.instrumental.Instrumentalist"
		p:song=" Jingle Bells" p:instrument-ref="saxophone">
	</bean>

	<bean id="hank" class="app.instrumental.OneManBand">
		<property name="instruments">
			<list>
				<ref bean="guitar" />
				<ref bean="cymbal" />
				<ref bean="harmonica" />
			</list>
		</property>
	</bean>

</beans>